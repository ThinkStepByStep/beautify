s1 = imread('sample1 copy.jpg');

[F, B] = facial(s1);

subplot(1, 4, 1);
s1b = insertShape(s1, "rectangle", B, "LineWidth", 10);
imshow(s1b);

subplot(1, 4, 2);
imshow(F);

subplot(1, 4, 3);
kernel_isolated_points = [-1 -1 -1; -1 8 -1; -1 -1 -1];
s1canny = (double(imfilter(im2gray(s1), kernel_isolated_points)) .* F) >= 80;
imshow(s s1canny);

subplot(1, 4, 4);
s1hsv = rgb2hsv(s1);
s1hsv(:, :, 2) = s1hsv(:, :, 2) * 0.7;
s1hsv(:, :, 3) = s1hsv(:, :, 3) * 1.1;
s1flat = hsv2rgb(s1hsv);
s1g = imblend(s1flat, s1, F);
imshow(s1g);